{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "packages/app/.svelte-kit": true,
    "packages/app/build": true,
    "**/packages/server/dist": true
  },
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/packages/server/dist": true
  },
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#ddd",
    "titleBar.inactiveForeground": "#ddd",
    "titleBar.activeBackground": "#254e4b",
    "titleBar.inactiveBackground": "#287b5f"
  }
}
